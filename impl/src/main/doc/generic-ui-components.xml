<!-- ========== Generic User Interface Components ==================================== -->



<root xmlns:xi="http://www.w3.org/2001/XInclude">

    <component>
        <component-type>javax.faces.Column</component-type>
        <component-class>javax.faces.component.UIColumn</component-class>

            <facet-name>header</facet-name>
        </facet>
        <facet>
            <facet-name>footer</facet-name>
        </facet>

        <xi:include href="uicolumn-props.xml" xpointer="xpointer(/root/*)" />

        <component-extension>
            <component-family>javax.faces.Column</component-family>
        </component-extension>
    </component>


